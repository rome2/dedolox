<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MainActivity">

    <GridLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:columnCount="8"
        android:rowCount="1"
        android:padding="0dp"
        android:useDefaultMargins="false"
        android:id="@+id/gridLayout"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="32dp"
            android:layout_height="wrap_content"
            android:id="@+id/button"
            android:layout_below="@+id/textView"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignRight="@+id/button2"
            android:layout_alignEnd="@+id/button2"
            android:layout_row="0"
            android:layout_column="0"
            android:layout_margin="0dp"
            android:width="32dp"/>

        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="32dp"
            android:layout_height="wrap_content"
            android:id="@+id/button2"
            android:layout_below="@+id/button"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_row="0"
            android:layout_column="1"/>

        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="32dp"
            android:layout_height="wrap_content"
            android:id="@+id/button3"
            android:layout_below="@+id/button2"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_row="0"
            android:layout_column="2"/>

        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="32dp"
            android:layout_height="wrap_content"
            android:id="@+id/button4"
            android:layout_below="@+id/button3"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_row="0"
            android:layout_column="3"/>

        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="32dp"
            android:layout_height="wrap_content"
            android:id="@+id/button5"
            android:layout_below="@+id/button4"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_row="0"
            android:layout_column="4"/>

        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="32dp"
            android:layout_height="wrap_content"
            android:id="@+id/button6"
            android:layout_below="@+id/button5"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_row="0"
            android:layout_column="5"/>

        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="32dp"
            android:layout_height="wrap_content"
            android:id="@+id/button7"
            android:layout_below="@+id/button6"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_row="0"
            android:layout_column="6"/>

        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="32dp"
            android:layout_height="wrap_content"
            android:id="@+id/button8"
            android:layout_below="@+id/button7"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_row="0"
            android:layout_column="7"
            android:layout_margin="0dp"
            android:padding="0dp"
            android:singleLine="false"
            android:width="1dp"/>
    </GridLayout>

    <GridLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_above="@+id/gridLayout"
        android:layout_alignRight="@+id/gridLayout"
        android:layout_alignEnd="@+id/gridLayout"
        android:columnCount="8"
        android:rowCount="2"
        android:id="@+id/gridLayout2">

        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="17dp"
            android:layout_height="wrap_content"
            android:id="@+id/button9"
            android:layout_alignTop="@+id/textView"
            android:layout_toRightOf="@+id/button"
            android:layout_toEndOf="@+id/button"
            android:background="#ffff6b1d"
            android:layout_row="0"
            android:layout_column="0"
            android:alpha="0"/>

        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="32dp"
            android:layout_height="wrap_content"
            android:id="@+id/button10"
            android:layout_below="@+id/button9"
            android:layout_alignLeft="@+id/button9"
            android:layout_alignStart="@+id/button9"
            android:layout_row="0"
            android:layout_column="1"/>

        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="32dp"
            android:layout_height="wrap_content"
            android:id="@+id/button11"
            android:layout_below="@+id/button10"
            android:layout_alignLeft="@+id/button9"
            android:layout_row="0"
            android:layout_column="2"/>

        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="32dp"
            android:layout_height="wrap_content"
            android:id="@+id/button12"
            android:background="#ff24ff09"
            android:layout_row="0"
            android:layout_column="3"
            android:layout_above="@+id/seekBar7"
            android:layout_toRightOf="@+id/editText2"
            android:layout_toEndOf="@+id/editText2"
            android:alpha="0"/>

        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="32dp"
            android:layout_height="wrap_content"
            android:id="@+id/button13"
            android:layout_below="@+id/button12"
            android:layout_alignLeft="@+id/button9"
            android:layout_row="0"
            android:layout_column="4"/>

        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="32dp"
            android:layout_height="wrap_content"
            android:id="@+id/button14"
            android:layout_below="@+id/button13"
            android:layout_alignLeft="@+id/button9"
            android:layout_row="0"
            android:layout_column="5"/>

        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="32dp"
            android:layout_height="wrap_content"
            android:id="@+id/button15"
            android:layout_below="@+id/button14"
            android:layout_alignLeft="@+id/button9"
            android:layout_row="0"
            android:layout_column="6"/>
    </GridLayout>

    <GridLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:columnCount="2"
        android:id="@+id/gridLayout3"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="VOL"
            android:id="@+id/textView"
            android:layout_row="0"
            android:layout_column="0"/>

        <SeekBar
            android:layout_width="260dp"
            android:layout_height="wrap_content"
            android:id="@+id/seekBar"
            android:max="127"
            android:progress="100"
            android:layout_toLeftOf="@+id/seekBar9"
            android:layout_toStartOf="@+id/seekBar9"
            android:layout_row="0"
            android:layout_column="1"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="A"
            android:id="@+id/textView2"
            android:layout_row="1"
            android:layout_column="0"/>

        <SeekBar
            android:layout_width="260dp"
            android:layout_height="wrap_content"
            android:id="@+id/seekBar2"
            android:max="127"
            android:layout_below="@+id/seekBar6"
            android:layout_toLeftOf="@+id/seekBar6"
            android:layout_toStartOf="@+id/seekBar6"
            android:layout_row="1"
            android:layout_column="1"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="H"
            android:id="@+id/textView3"
            android:layout_row="2"
            android:layout_column="0"/>

        <SeekBar
            android:layout_width="260dp"
            android:layout_height="wrap_content"
            android:id="@+id/seekBar3"
            android:max="127"
            android:layout_above="@+id/editText6"
            android:layout_alignRight="@+id/seekBar"
            android:layout_alignEnd="@+id/seekBar"
            android:layout_row="2"
            android:layout_column="1"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="D"
            android:id="@+id/textView4"
            android:layout_row="3"
            android:layout_column="0"/>

        <SeekBar
            android:layout_width="260dp"
            android:layout_height="wrap_content"
            android:id="@+id/seekBar4"
            android:max="127"
            android:layout_below="@+id/editText6"
            android:layout_alignLeft="@+id/editText7"
            android:layout_alignStart="@+id/editText7"
            android:layout_row="3"
            android:layout_column="1"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="S"
            android:id="@+id/textView5"
            android:layout_row="4"
            android:layout_column="0"/>

        <SeekBar
            android:layout_width="260dp"
            android:layout_height="wrap_content"
            android:id="@+id/seekBar5"
            android:max="127"
            android:layout_below="@+id/seekBar7"
            android:layout_toRightOf="@+id/editText6"
            android:layout_toEndOf="@+id/editText6"
            android:layout_row="4"
            android:layout_column="1"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="R"
            android:id="@+id/textView6"
            android:layout_row="5"
            android:layout_column="0"/>

        <SeekBar
            android:layout_width="260dp"
            android:layout_height="wrap_content"
            android:id="@+id/seekBar6"
            android:max="127"
            android:layout_above="@+id/seekBar8"
            android:layout_toRightOf="@+id/seekBar5"
            android:layout_toEndOf="@+id/seekBar5"
            android:layout_below="@+id/seekBar7"
            android:layout_row="5"
            android:layout_column="1"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="LFO1"
            android:id="@+id/textView7"
            android:layout_row="6"
            android:layout_column="0"/>

        <SeekBar
            android:layout_width="260dp"
            android:layout_height="wrap_content"
            android:id="@+id/seekBar7"
            android:max="127"
            android:layout_below="@+id/editText5"
            android:layout_toRightOf="@+id/editText6"
            android:layout_alignRight="@+id/seekBar6"
            android:layout_alignEnd="@+id/seekBar6"
            android:layout_row="6"
            android:layout_column="1"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="FREQ"
            android:id="@+id/textView8"
            android:layout_row="7"
            android:layout_column="0"/>

        <SeekBar
            android:layout_width="260dp"
            android:layout_height="wrap_content"
            android:id="@+id/seekBar8"
            android:max="127"
            android:layout_alignParentTop="true"
            android:layout_toRightOf="@+id/seekBar7"
            android:layout_toEndOf="@+id/seekBar7"
            android:layout_row="7"
            android:layout_column="1"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="RES"
            android:id="@+id/textView9"
            android:layout_row="8"
            android:layout_column="0"/>

        <SeekBar
            android:layout_width="260dp"
            android:layout_height="wrap_content"
            android:id="@+id/seekBar9"
            android:max="127"
            android:layout_alignBottom="@+id/editText7"
            android:layout_alignRight="@+id/gridLayout3"
            android:layout_alignEnd="@+id/gridLayout3"
            android:layout_row="8"
            android:layout_column="1"/>

    </GridLayout>

</RelativeLayout>
